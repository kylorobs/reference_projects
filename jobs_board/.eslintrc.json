{
  "extends": [
    "wesbos/typescript",
    "next/core-web-vitals"
  ],
  "rules": {
    "react/destructuring-assignment": 1,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "no-return-assign": 0,
    "no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "^_*|/res|next|^err/u"
      }
    ],
    "prettier/prettier": [
      1,
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}